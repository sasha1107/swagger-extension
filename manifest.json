{
    "name": "Toggle Swagger",
    "manifest_version": 3,
    "version": "0.1",
    "description": "Open all Tabs in Swagger UI",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+T",
                "mac": "Alt+T"
            }
        }
    }
}
