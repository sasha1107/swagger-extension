{
    "name": "Toggle Swagger",
    "action": {},
    "manifest_version": 3,
    "version": "0.1",
    "description": "Open all Tabs in Swagger UI",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+T",
                "mac": "Alt+T"
            }
        }
    }
}
